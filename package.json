{
  "name": "grapherjs",
  "version": "0.0.11",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "test": "export NODE_ENV=test && jest --runInBand --watchAll --no-cache",
    "build": "yarn prebuild && yarn build:tsc && yarn postbuild",
    "build:tsc": "tsc --project ./tsconfig.json && tsc-alias -p ./tsconfig.json",
    "commit": "cz",
    "postbuild": "cp package.json README.md dist",
    "prebuild": "rimraf tsconfig.tsbuildinfo && rimraf dist && rimraf coverage",
    "release": "standard-version"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.17.3",
    "jwt-decode": "^3.1.2",
    "lodash.isempty": "^4.4.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "request-ip": "^2.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/axios": "^0.14.0",
    "@types/cookie": "^0.5.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.pick": "^4.4.7",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.36",
    "commitizen": "^4.2.4",
    "conventional-github-releaser": "^3.1.5",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "^6.2.3",
    "ts-jest": "27.1.4",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "typescript": "^4.6.4"
  }
}
